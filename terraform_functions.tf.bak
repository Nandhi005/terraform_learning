provider "aws" {
    region = var.region
}

resource "aws_instance" "example" {
  ami           = lookup(var.ami_id,var.region)
  instance_type = "t2.micro"
  vpc_security_group_ids = ["sg-0769c72cb8873b4d8"]
  count = length(var.instance_name)
  tags = {
    Name = element(var.instance_name,count.index)
  }
}