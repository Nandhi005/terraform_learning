terraform {
  required_providers {
    github = {
      source  = "integrations/github"
      version = "~> 6.0"
    }
  }
}
provider "github" {
  token = ""`GITHUB_TOKEN`
}
resource "github_repository" "example" {
  name        = "terraform_example"
  description = "My awesome codebase"

  visibility = "public"
}
